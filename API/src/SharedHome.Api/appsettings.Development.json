{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId", "WithEnvironmentName" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341/"
        }
      }
    ]
  },
  "General": {
    "ApiName": "SharedHome",
    "ConfirmationEmailAngularClient": "http://localhost:4200/emailconfirmation?email={0}&code={1}"
  },
  "MySQL": {
    "ConnectionString": ""
  },
  "Jwt": {
    "Secret": "",
    "Expiry": "07.00:00:00",
    "Issuer": "SharedHome",
    "Audience": "SharedHome"
  },
  "Email": {
    "Host": "smtp.gmail.com",
    "Port": 465,
    "Address": "",
    "Password": ""
  },
  "Initializer": {
    "AdminPassword": "",
    "CharlesPassword": "",
    "FrancPassword": ""
  },
  "Seq": {
    "Uri": "http://localhost:5341"
  },
  "SignalR": {
    "NotificationsUri": "https://localhost:7073/notify",
    "NotificationsPattern": "/notify"
  }
}
